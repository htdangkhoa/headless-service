{
  "name": "headless-service",
  "version": "1.0.0",
  "description": "Headless Service is a Browser as a Service (BaaS) that provides a way to run browser automation tasks without the need to install a browser on the client machine.",
  "keywords": [
    "nodejs",
    "docker",
    "typescript",
    "websocket",
    "headless",
    "browser",
    "puppeteer",
    "playwright",
    "automation"
  ],
  "license": "ISC",
  "author": "",
  "type": "module",
  "scripts": {
    "prebuild": "rimraf dist && pnpm run build:all",
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "tsc-alias -p tsconfig.build.json -f true -fe .js",
    "build:all": "pnpm run /^build:.*/ & pnpm run /^install:.*/",
    "build:extensions": "node scripts/build/extensions.js",
    "build:function": "node scripts/build/function.js",
    "build:live": "node scripts/build/live.js",
    "predev": "pnpm run build:all",
    "dev": "nodemon",
    "format": "prettier --write \"**/*.{js,ts,tsx,md}\"",
    "install:devtools": "node scripts/install/devtools.js",
    "lint": "tsc --noEmit",
    "plugin:create": "node scripts/plugin/create.js",
    "start": "node dist/index.js",
    "tsx": "tsx"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "8.1.0",
    "@ghostery/adblocker-puppeteer": "2.12.4",
    "connect-timeout": "1.9.1",
    "cors": "2.8.5",
    "dayjs": "1.11.18",
    "debug": "4.4.3",
    "dedent": "1.7.0",
    "devtools-protocol": "0.0.1528500",
    "express": "5.1.0",
    "fingerprint-generator": "2.1.75",
    "fingerprint-injector": "2.1.75",
    "glob": "11.0.3",
    "jsonwebtoken": "9.0.2",
    "lighthouse": "13.0.0",
    "lodash-es": "4.17.21",
    "lucide-static": "0.545.0",
    "proxy-chain": "2.5.9",
    "puppeteer": "24.25.0",
    "puppeteer-core": "24.25.0",
    "puppeteer-extra": "3.3.6",
    "puppeteer-extra-plugin": "3.2.3",
    "puppeteer-extra-plugin-stealth": "2.11.2",
    "qs": "6.14.0",
    "rimraf": "6.0.1",
    "tree-kill": "1.2.2",
    "typescript": "5.9.3",
    "ws": "8.18.3",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.7.0",
    "@types/chrome": "0.1.24",
    "@types/connect-timeout": "1.9.0",
    "@types/consolidate": "0.14.4",
    "@types/cors": "2.8.19",
    "@types/debug": "4.1.12",
    "@types/express": "5.0.3",
    "@types/jsonwebtoken": "9.0.10",
    "@types/lodash-es": "4.17.12",
    "@types/node": "24.7.2",
    "@types/qs": "6.14.0",
    "@types/ws": "8.18.1",
    "esbuild": "0.25.11",
    "esbuild-plugin-polyfill-node": "0.3.0",
    "extract-zip": "2.0.1",
    "nodemon": "3.1.10",
    "prettier": "3.6.2",
    "tsc-alias": "1.8.16",
    "tsx": "4.20.6"
  },
  "optionalDependencies": {
    "bufferutil": "4.0.9"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=8.0.0"
  }
}
