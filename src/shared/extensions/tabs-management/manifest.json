{
  "name": "Tabs Management",
  "version": "0.0.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ],
  "host_permissions": ["<all_urls>"],
  "permissions": ["tabs", "scripting"]
}
