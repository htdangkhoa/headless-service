{
  "name": "Video Capture",
  "version": "0.0.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"],
      "run_at": "document_start"
    }
  ],
  "externally_connectable": {
    "matches": ["<all_urls>"]
  },
  "permissions": ["desktopCapture", "<all_urls>", "downloads"]
}
